{
  "family": "flask-app-new",
  "containerDefinitions": [
    {
      "name": "flask-app-new",
      "essential": true,
      "image": "%ECR_IMAGE%",
      "memoryReservation": 2000,
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 5000,
          "hostPort": 5000
        }
      ]
    }
  ]
}